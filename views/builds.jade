extends layout

block content
  p server: #{server} filter: #{prefix}

  ul
      each build in builds
          li
            h3 #{build.projectName} :: #{build.name} :: #{build.latestBuild.status}
            img(src='http://#{server}/app/rest/builds/buildType:(id:#{build.latestBuild.buildTypeId})/statusIcon')
            | #{build.latestBuild.id}


